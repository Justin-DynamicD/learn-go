// For format details, see https://aka.ms/vscode-remote/devcontainer.json or this file's README at:
// https://github.com/microsoft/vscode-dev-containers/tree/v0.128.0/containers/docker-existing-dockerfile
{
	"name": "Existing Dockerfile",
	"context": ".",
	"dockerFile": "Dockerfile",
	"workspaceMount": "source=${localWorkspaceFolder},target=/workspace/src,type=bind,consistency=delegated",
  "workspaceFolder": "/workspace/src",
	"settings": { 
		"terminal.integrated.shell.linux": null
	},
	"extensions": ["golang.go"]
}
